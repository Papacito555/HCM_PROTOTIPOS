<script type="text/javascript">
$("#datepickerInicio").datepicker({
    format: 'mm/dd/yyyy',
    startDate: '-3d',
    locale: 'es-es',
    todayHighlight: true,
    autoclose: true,
    language: 'es'
   
});
$("#datepickerFin").datepicker({
    format: 'mm/dd/yyyy',
    startDate: '-3d',
    locale: 'es-es',
    todayHighlight: true,
    autoclose: true,
    language: 'es'
   
});
$(document).ready(function() {

        $('#borrar').on('show.bs.modal', function(event) { 
                var modal = $(this);
                var referencia = $(event.relatedTarget) // Botón que activó el modal
                var idSelccionado=referencia.data().id;
                var nombre=$('#data-nombre'+idSelccionado).text();
                var asunto=$('#data-asunto'+idSelccionado).val();
                var descCita=$('#data-descripcionCita'+idSelccionado).val();
                var fecha=$('#data-fecha'+idSelccionado).text();
                var hora=$('#data-hora'+idSelccionado).text();
                //console.log("nombre "+nombre);
                //console.log("asunto "+asunto);
                //console.log("fceha "+fecha);
                //var idCita=$('#data-id'+idSelccionado).val();
                modal.find('.modal-body #txtNombreCliente').val(nombre)
                modal.find('.modal-body #txtAsunto').val(descCita)
                modal.find('.modal-body #txtFechaInicio').val(fecha+" "+hora)
                modal.find('.modal-body #ocultoIDEliminar').val(idSelccionado);
        });
        $('#ver').on('show.bs.modal', function(event) { 
                var modal = $(this);
                var referencia = $(event.relatedTarget) // Botón que activó el modal
                var idSelccionado=referencia.data().id;
                var horario=$('#data-hora'+idSelccionado).text();
                var nombre=$('#data-nombre'+idSelccionado).text();
                var curp=$('#data-curp'+idSelccionado).text();
                var asunto=$('#data-asunto'+idSelccionado).val();
                var descCita=$('#data-descripcionCita'+idSelccionado).val();
                var tramite=$('#data-tramite'+idSelccionado).val();
                var seccion=$('#data-seccion'+idSelccionado).val();
                var fecha=$('#data-fecha'+idSelccionado).text();
                var hora=$('#data-hora'+idSelccionado).text();
                var horafin=$('#data-horafin'+idSelccionado).text();
                //console.log("nombre "+nombre);
                //console.log("asunto "+asunto);
                //console.log("fceha "+fecha);
                //console.log("tramite "+tramite);
                //console.log("asunto de seccion "+descCita)
                //console.log("seccion "+seccion);
                var telefonousuario=$('#data-teleusuario'+idSelccionado).val();
                modal.find('.modal-body #txtTelefono').val(telefonousuario)
                //var idCita=$('#data-id'+idSelccionado).val();
                modal.find('.modal-body #txtNombreCliente').val(nombre)
                modal.find('.modal-body #txtAsunto').val(descCita)
                modal.find('.modal-body #txtTramite').val(tramite)
                modal.find('.modal-body #txtSeccion').val(seccion)
                modal.find('.modal-body #txtCurp').val(curp)
                modal.find('.modal-body #txtFechaInicio').val(fecha+", Horario: "+horario)
                modal.find('.modal-body #txtFechaFin').val(fecha+" "+horafin)
                modal.find('.modal-body #ocultoIDEliminar').val(idSelccionado);
        });
        $('#modalEdicionCita').on('show.bs.modal', function(event) { 
                var esPersonaMoral="";
                var modal = $(this);
                var referencia = $(event.relatedTarget) // Botón que activó el modal
                var idSelccionado=referencia.data().id;
                var idUsuario=$('#data-idusuario'+idSelccionado).val();
                var nombre=$('#data-nombre'+idSelccionado).text();
                var curp=$('#data-curp'+idSelccionado).text();
                var asunto=$('#data-asunto'+idSelccionado).val();
                var descCita=$('#data-descripcionCita'+idSelccionado).val();
                var tramite=$('#data-tramite'+idSelccionado).val();
                var seccion=$('#data-seccion'+idSelccionado).val();
                var razons=$('#data-razonsocial'+idSelccionado).val();
                var comentarios=$('#data-comentarios'+idSelccionado).val();
                esPersonaMoral=$('#data-personamoral'+idSelccionado).val();
                var edo=$('#data-edo'+idSelccionado).val();
                var fecha=$('#data-fecha'+idSelccionado).text();
                var horario=$('#data-hora'+idSelccionado).text();
                //var horafin=$('#data-horafin'+idSelccionado).text();
                //console.log("id cita editar  "+idSelccionado);
                //console.log("id usuario editar  "+idUsuario);
                //console.log("nombre "+nombre);
                //console.log("curp "+curp);
                //console.log("asunto "+asunto);
                //console.log("fceha "+fecha);
                //console.log("tramite "+tramite);
                //console.log("seccion "+seccion);
                //console.log("horario "+horario);
                //console.log("es persona moral ? "+esPersonaMoral);
                //console.log("razon social "+razons);
                //console.log("comentarios "+comentarios);
                //console.log("asunto de seccion/ nuevo Tramite "+descCita);
                //var idCita=$('#data-id'+idSelccionado).val();
                modal.find('.modal-body #txtNombreCliente2').val(nombre)
                modal.find('.modal-body #txtAsunto2').val(asunto)
                modal.find('.modal-body #txtTramite2').val(tramite)
                modal.find('.modal-body #description2').val(descCita);
                modal.find('.modal-body #txtSeccion2').val(seccion)
                modal.find('.modal-body #txtCurp2').val(curp)
                modal.find('.modal-body #txtFechaInicio2').val(fecha+" "+horario)
                //modal.find('.modal-body #txtFechaFin').val(fecha+" "+horafin)
                modal.find('.modal-body #txtEdoCita2').val(edo)
                modal.find('.modal-body #txtRZ2').val(razons)
                modal.find('.modal-body #comentarios').val(comentarios)
                if(esPersonaMoral==="true"){
                    document.getElementById('checkPM2').checked = true;
                     document.getElementById('checkPM2').value=true;
                    $("#etiRazonSocial2").css("display", "block");
                       // console.log("es persona moral");
                }else{
                    document.getElementById('checkPM2').checked = false;
                    document.getElementById('checkPM2').value=false;
                    $("#etiRazonSocial2").css("display", "none");
                    //console.log("NO es persona moral");
                }
                //modal.find('.modal-body #ocultoIDEditar').val(idSelccionado);
                modal.find('.modal-body #idcita2').val(idSelccionado);
                modal.find('.modal-body #idUsuario').val(idUsuario);
                modal.find('.modal-body #ocultoCurp').val(curp);
        });
$('.submit_on_enter').keydown(function (e){
           // Capturamos qué telca ha sido
        //alert("Hola");
        var keyCode= e.which;
        // Si la tecla es el Intro/Enter
            if (keyCode == 13){
                if(document.getElementById('txtUsuario').value.length<=0){
                $('#messageModalError4').modal('show');
                return false;
            }else{
                //document.getElementById('f1').action="buscarCitaSegunNombreCliente";
                //document.getElementById('f1').submit();
                //alert("hizo submit");
            
            }
                
                // Evitamos que se ejecute eventos
                   // event.preventDefault();
                // Devolvemos falso
                //return false;
        }
        
       
});

});
function onTimeChange() {
  var inputEle = document.getElementById('starttime2').value;
  //alert("valor "+inputEle);
  var timeSplit = inputEle.split(':');
  var hours="";
  var minutes="";
  var meridian="";
  hours = timeSplit[0];
  minutes = timeSplit[1];
  if (hours > 12) {
    meridian = 'PM';
    hours -= 12;
  } else if (hours < 12) {
    meridian = 'AM';
    if (hours === 0) {
      hours = 12;
    }
  } else {
    meridian = 'PM';
  }
 // console.log("meridiano es "+meridian)
  document.getElementById('ocultoampm2').value=meridian;
  document.getElementById('horaCita2').value=hours;
  //alert(hours + ':' + minutes + ' ' + meridian);
}
function mostrarMsgProcesoEdicion(){
    var tipomsg2=document.getElementById('msgProcesoEdicionCita').value;
    if(document.getElementById('btnConitnuar').disabled){
             document.getElementById('btnConitnuar').disabled=false;
    }  
    //console.log("tipo de mensajes en edicion cita "+tipomsg2);
    if(tipomsg2=="om2"){//error edicion
        $('#messageModalError2').modal('show');
        document.getElementById('msgProcesoEdicionCita').value="";
    }
    if(tipomsg2=="ok2"){//error
                        //alert("ojo");
        $('#messageModalExito2').modal('show');
        document.getElementById('msgProcesoEdicionCita').value="";
    }
    
}
function desactivarContinuar2(){
         document.getElementById('f2').submit();
         document.getElementById('btnConitnuar').disabled=true;
}
function mostrarModalEdicionCita(){
     $('#modalEdicionCita').modal('show'); 
}
function limpiar(){
    //alert("limpio");
    document.getElementById('txtUsuario').value="";
    document.getElementById('txtCurp').value="";
    document.getElementById('txtFechaInicio').value="";
    document.getElementById('txtFechaFin').value="";
    document.getElementById('f1').action="resetBusquedaCitaNormal";
    document.getElementById('f1').submit();
}
$(document).ready(function() {
 $("#cmbSeccion").change(function() {
       // alert("id seccion "+$("#cmbSeccion").val());
        $("#cmbAsunto").empty();
        $.ajax({
                type: "GET",
                url: './buscarAsunto?id='+$("#cmbSeccion").val(),
                success: function(response)
                {
                    $("#cmbAsunto").html(response).fadeIn();
                }
      });
 });
 $("#cmbAsunto").change(function() {
        //alert("id del asunto a buscar "+$("#cmbAsunto").val());
       var fecha_consulta=document.getElementById('txtFechaInicio').value;
       //console.log("fecha de consulta de horario "+fecha_consulta);
       //console.log("dia consulta de horario "+fecha_consulta.substring(8, 10));
        $("#cmbHora").empty();
        $.ajax({
                type: "GET",
                url: './buscarHorario?idAsunto='+$("#cmbAsunto").val()+'&dia='+fecha_consulta.substring(8, 10),
                success: function(response)
                {
                    $("#cmbHora").html(response).fadeIn();
                }
      });
 });
 $("#cmbAsunto2").change(function() {
        //alert("id del asunto a buscar "+$("#cmbAsunto").val());
        var fecha_consulta2=document.getElementById('txtFechaInicio2').value;
        //console.log("fecha de consulta de horario "+fecha_consulta2);
        //console.log("dia consulta de horario "+fecha_consulta2.substring(8, 10));
        $("#cmbHora2").empty();
        $.ajax({
                type: "GET",
                url: './buscarHorario?idAsunto='+$("#cmbAsunto2").val()+'&dia='+fecha_consulta2.substring(8, 10),
                success: function(response)
                {
                    $("#cmbHora2").html(response).fadeIn();
                }
      });
 });
    $("#cmbSeccion2").change(function() {
       // alert("id seccion "+$("#cmbSeccion").val());
        $("#cmbAsunto2").empty();
        $.ajax({
            type: "GET",
            url: './buscarAsunto?id='+$("#cmbSeccion2").val(),
            success: function(response)
            {
                $("#cmbAsunto2").html(response).fadeIn();
            }
      });
 
    });
    
    
});
function mostrarRazonSocial(){
   // alert("a" );
    var checkbox = document.getElementById('checkPM');
    checkbox.addEventListener( 'change', function() {
    if(this.checked) {
       //alert('checkbox esta seleccionado');
       $("#etiRazonSocial").css("display", "block");
       document.getElementById('checkPM').value=true;
    }else{
        $("#etiRazonSocial").css("display", "none");
        document.getElementById('checkPM').value=false;
    }
});
}
function mostrarRazonSocial2(){
   // alert("a" );
    var checkbox = document.getElementById('checkPM2');
    checkbox.addEventListener( 'change', function() {
    if(this.checked) {
       //alert('checkbox esta seleccionado');
       $("#etiRazonSocial2").css("display", "block");
       document.getElementById('checkPM2').value=true;
    }else{
        $("#etiRazonSocial2").css("display", "none");
        document.getElementById('checkPM2').value=false;
    }
});
}
</script>
 
