   /*** INICIO DE NUEVO CALENDAR ****/
   function addZero(i) {
    if (i < 10) {
        i = '0' + i;
    }
    return i;
}

var hoy = new Date();
var dd = hoy.getDate();
if(dd<10) {
    dd='0'+dd;
} 
 
if(mm<10) {
    mm='0'+mm;
}

var mm = hoy.getMonth()+1;
var yyyy = hoy.getFullYear();

dd=addZero(dd);
mm=addZero(mm);
var fechaActual=yyyy+"-"+mm+"-"+dd;
var idEvento="";
var idEventoNro=0;
var seccion="";
var tramite="";
var fechaHora="";
var edoReunion="";
var descReunion="";
var esPersonaMoral="";
var razonSoncial="";
var comentarios="";
var idHorario="";
var fechaSeleccionada="";
$(document).ready(function() {
    var dataCitas="";
    var results = document.getElementById('arrcitasJSON').value;
    //console.log("valor de results "+results);
    var myArrayJson = eval('(' + results + ')');
    var f = new Date();
    var dd = hoy.getDate();
    var mm = hoy.getMonth()+1;
    var yyyy = hoy.getFullYear();
    if(dd<10) {
        dd='0'+dd;
    }    
    if(mm<10) {
        mm='0'+mm;
    } 
    dataCitas=JSON.parse(JSON.stringify(myArrayJson));
    //console.log("data de citas final "+dataCitas);
    $('#fc-selectable').fullCalendar({
        header: {
            left: 'prev,next Hoy',
            center: 'title',
             right: ''
            //right: 'month,agendaWeek,agendaDay'
        },
        /*validRange: { habilita rango valido de fechas a visualizar en calenadrio
            start: yyyy+"-"+mm+"-"+dd
        },*/
        //defaultDate: yyyy+'-'+mm+'-'+dd,
        buttonIcons: true, // show the prev/next text
        weekNumbers: false,
        editable: true,
        defaultDate: fechaActual,
        eventLimit: true, // allow "more" link when too many events 
        events: dataCitas,
        weekends: false,//muestra o no los findes de semana
        //backgroundColor: '#ff7216',
        selectable: true,
        //plugins: ["dayGrid", "timeGrid", "interaction"],
        selectHelper: true,
        /*select: function(arg) {//permite selecciona una fecha y mostrarla en modal
          // $('#modalRegistroCita').modal('show',{backdrop: 'static', keyboard: false}); 
          jQuery('#modalRegistroCita').modal({backdrop: 'static', keyboard: false});
          //console.log("valores en select "+idEventoNro);
           //fcSelectable.unselect()
        },*/
        dayClick: function (date, jsEvent, view, calEvent) {
            //alert('Has hecho click en: '+ date.format());
            fechaSeleccionada=date.format();
            if(document.getElementById('ocultoFechaSeleccionada').value==="nuevaseleccion"){
                //console.log("fecha seleccionada "+fechaSeleccionada);
              // console.log("no hay asignacion a campo oculto ");
                //document.getElementById('ocultoFechaSeleccionada').value="vacio";
            }else{
                document.getElementById('ocultoFechaSeleccionada').value=fechaSeleccionada;
                //console.log("fecha seleccionada "+fechaSeleccionada+" hubo asignacion");
            }
            
            
            
        },
        eventClick: function (calEvent, jsEvent, view) {
        //info.jsEvent.preventDefault(); // don't let the browser navigate
        //alert('Event id : ' + calEvent.id);
        idEvento=calEvent.id;
        //console.log("valor de id del evento "+idEvento);
        var arrayDeCadenas = idEvento.split("|");
           // console.log("valor de id del evento2 "+calEvent.id);
            idEventoNro=arrayDeCadenas[0];
           // console.log("valor de id del evento luego de formato "+idEventoNro);
            tramite=arrayDeCadenas[1];
            seccion=arrayDeCadenas[2];
            fechaHora=arrayDeCadenas[3];
            descReunion=arrayDeCadenas[4];
            edoReunion=arrayDeCadenas[5];
            esPersonaMoral=arrayDeCadenas[6];
            razonSoncial=arrayDeCadenas[7];
            comentarios=arrayDeCadenas[8];
            idHorario=arrayDeCadenas[9];
            //console.log("valor de seccion del evento "+seccion);
            if(idEventoNro.includes('$')){
                $('#messageModalErrorDNP').modal('show');                
            }else{
                if(seccion==="vacio"){
                    if(document.getElementById('ocultoFechaSeleccionada').value===fechaSeleccionada){
                      document.getElementById('ocultoFechaSeleccionada').value="nuevaseleccion";
                      $('#messageModalErrorDNP2').modal('show');  
                    }else{
                        jQuery('#modalRegistroCita').modal({backdrop: 'static', keyboard: false}); 
                    } 
                }else{
                    if (calEvent.url) {
                    //window.open(info.event.url);
                    //$('#modalEdicionCita').modal('show',{backdrop: 'static', keyboard: false});
                        //jQuery('#modalEdicionCita').modal({backdrop: 'static', keyboard: false});
                        console.log("se solicito inhabilitar la edicion de citas");
                    }    
                    
                }
               
            }
        /*eventClick: function (calEvent, jsEvent, view) {
            $('#event-title').text(calEvent.title);
            $('#event-description').html(calEvent.description);
            $('#modal-event').modal();
        },*/
        }
 	});
            //fcSelectable.render();
    $('#external-events .fc-event').each(function () {
        // Different colors for events
        $(this).css({ 'backgroundColor': $(this).data('color'), 'borderColor': $(this).data('color') });
    });
    var colorData;
    $('#external-events .fc-event').mousemove(function(){
    colorData = $(this).data('color');
    });
    
    
});

  /***FIN DE NUEVO CALENDAR ****/      
  $(document).ready(function (e) {
        $('#modalRegistroCita').on('show.bs.modal', function(e) {    
                        //var id = $(e.relatedTarget).data().id;
                            //console.log("valor de id en registro cita "+id);
                        //$(e.currentTarget).find('#lista').val(id);
                        //alert('Event id2 : ' + idEvento); 
                         //var id = $(e.relatedTarget).data().id;
                        //$(e.currentTarget).find('.modal-body #nombrecustomer_id').val("hola "+idEvento);
                        //console.log("abriendo registro, idEvento "+idEventoNro);
                        $(e.currentTarget).find('.modal-body #txtSeccReg').val(seccion);
                        $(e.currentTarget).find('.modal-body #txtFechaInicio').val(fechaSeleccionada);
        
        });
        $('#modalEdicionCita').on('show.bs.modal', function(e) {    
            //var id = $(e.relatedTarget).data().id;
            //$(e.currentTarget).find('#lista').val(id);
            //alert('Event id2 : ' + idEvento); 
            //var id = $(e.relatedTarget).data().id;
            $(e.currentTarget).find('.modal-body #idcita2').val(idEventoNro);
            $(e.currentTarget).find('.modal-body #txtTramReg2').val(tramite+" "+idEventoNro);
            $(e.currentTarget).find('.modal-body #txtSeccReg2').val(seccion);
            $(e.currentTarget).find('.modal-body #txtFechaReg2').val(fechaHora);
            $(e.currentTarget).find('.modal-body #description2').val(descReunion);
            $(e.currentTarget).find('.modal-body #txtEdoCita2').val(edoReunion);
            //$(e.currentTarget).find('.modal-body #txtFechaInicio2').val(fechaSeleccionada);
            if(esPersonaMoral==="true"){
                document.getElementById('checkPM2').checked = true;
                document.getElementById('checkPM2').value=true;
                $("#etiRazonSocial2").css("display", "block");
                //console.log("es persona moral");
            }else{
                document.getElementById('checkPM2').checked = false;
                document.getElementById('checkPM2').value=false;
                $("#etiRazonSocial2").css("display", "none");
                //console.log("NO es persona moral");
            }
            //console.log("id del horario seleccionado "+idHorario);
            //$(e.currentTarget).find('.modal-body #txtEdoCita2').val(edoReunion);
            $(e.currentTarget).find('.modal-body #txtRZ2').val(razonSoncial);
            $(e.currentTarget).find('.modal-body #comentarios').val(comentarios);
            $(e.currentTarget).find('.modal-body #idHorarioCitaEditar').val(idHorario);
        
        });
  
});
function desactivarContinuar(){
         document.getElementById('f1').submit();
         document.getElementById('btnConitnuar').disabled=true;
}
function desactivarContinuar2(){
         document.getElementById('f2').submit();
         document.getElementById('btnConitnuar').disabled=true;
}
            function mostrarMsgProceo(){
                var tipomsg=document.getElementById('msgProcesoRegCita').value;
                /*
                if(document.getElementById('btnConitnuar').disabled){
                    document.getElementById('btnConitnuar').disabled=false;
                }*/
              //  console.log("tipo de mensajes en registro cita "+tipomsg);
                if(tipomsg=="om"){//error
                    $('#messageModalError1').modal('show');
                    document.getElementById('msgProcesoRegCita').value="";
                }
           
                if(tipomsg=="ok"){//error
                    $('#messageModalExito').modal('show');
                    document.getElementById('msgProcesoRegCita').value="";
                }
            }
                function mostrarMsgProcesoEdicion(){
                   var tipomsg2=document.getElementById('msgProcesoEdicionCita').value;
                //    console.log("tipo de mensajes en edicion cita "+tipomsg2);
                 if(tipomsg2=="om2"){//error edicion
                        $('#messageModalError2').modal('show');
                        document.getElementById('msgProcesoEdicionCita').value="";
                    }
                    if(tipomsg2=="ok2"){//error
                        //alert("ojo");
                        $('#messageModalExito2').modal('show');
                        document.getElementById('msgProcesoEdicionCita').value="";
                    }
                }
function mostrarModalRegistro(){
     $('#modalRegistroCita').modal('show'); 
}

function mostrarModalEdicionCita(){
     $('#modalEdicionCita').modal('show'); 
}
function mostrarRazonSocial(){
   // alert("a" );
    var checkbox = document.getElementById('checkPM');
    checkbox.addEventListener( 'change', function() {
    if(this.checked) {
       //alert('checkbox esta seleccionado');
       $("#etiRazonSocial").css("display", "block");
       document.getElementById('checkPM').value=true;
    }else{
        $("#etiRazonSocial").css("display", "none");
        document.getElementById('checkPM').value=false;
    }
});
}

function mostrarRazonSocial2(){
   // alert("a" );
    var checkbox = document.getElementById('checkPM2');
    checkbox.addEventListener( 'change', function() {
    if(this.checked) {
       //alert('checkbox esta seleccionado');
       $("#etiRazonSocial2").css("display", "block");
       document.getElementById('checkPM2').value=true;
    }else{
        $("#etiRazonSocial2").css("display", "none");
        document.getElementById('checkPM2').value=false;
    }
});
}

function mostrarComentarios(){
   // alert("a" );
    var checkbox = document.getElementById('checkCom');
    checkbox.addEventListener( 'change', function() {
    if(this.checked) {
       //alert('checkbox esta seleccionado');
       $("#etiComentarios").css("display", "block");
    }else{
        $("#etiComentarios").css("display", "none");
    }
});
}   
    


function onTimeChange() {
  var inputEle = document.getElementById('starttime1').value;
  //alert("valor "+inputEle);
  var timeSplit = inputEle.split(':');
  var hours="";
  var minutes="";
  var meridian="";
  hours = timeSplit[0];
  minutes = timeSplit[1];
  if (hours > 12) {
    meridian = 'PM';
    hours -= 12;
  } else if (hours < 12) {
    meridian = 'AM';
    if (hours === 0) {
      hours = 12;
    }
  } else {
    meridian = 'PM';
  }
  //console.log("meridiano es "+meridian)
  document.getElementById('ocultoampm').value=meridian;
  document.getElementById('horaCita').value=hours;
  //alert(hours + ':' + minutes + ' ' + meridian);
}
function onTimeChange2() {
  var inputEle = document.getElementById('starttime2').value;
  //alert("valor "+inputEle);
  var timeSplit = inputEle.split(':');
  var hours="";
  var minutes="";
  var meridian="";
  hours = timeSplit[0];
  minutes = timeSplit[1];
  if (hours > 12) {
    meridian = 'PM';
    hours -= 12;
  } else if (hours < 12) {
    meridian = 'AM';
    if (hours === 0) {
      hours = 12;
    }
  } else {
    meridian = 'PM';
  }
  //console.log("meridiano es "+meridian)
  document.getElementById('ocultoampm2').value=meridian;
  document.getElementById('horaCita2').value=hours;
  //alert(hours + ':' + minutes + ' ' + meridian);
}
function addZero(i) {
    if (i < 10) {
        i = '0' + i;
    }
    return i;
}

$(document).ready(function() {
 $("#cmbSeccion").change(function() {
       // alert("id seccion "+$("#cmbSeccion").val());
       
        $("#cmbAsunto").empty();
        $.ajax({
                type: "GET",
                url: './buscarAsunto?id='+$("#cmbSeccion").val(),
                success: function(response)
                {
                    $("#cmbAsunto").html(response).fadeIn();
                }
      });
 });
 $("#cmbAsunto").change(function() {
        //alert("id del asunto a buscar "+$("#cmbAsunto").val());
       var fecha_consulta=document.getElementById('txtFechaInicio').value;
       //console.log("fecha de consulta de horario "+fecha_consulta);
       //console.log("dia consulta de horario "+fecha_consulta.substring(8, 10));
        $("#cmbHora").empty();
        $.ajax({
                type: "GET",
                url: './buscarHorario?idAsunto='+$("#cmbAsunto").val()+'&dia='+fecha_consulta.substring(8, 10),
                success: function(response)
                {
                    $("#cmbHora").html(response).fadeIn();
                }
      });
 });
 $("#cmbAsunto2").change(function() {
        //alert("id del asunto a buscar "+$("#cmbAsunto").val());
        var fecha_consulta2=document.getElementById('txtFechaInicio2').value;
        //console.log("fecha de consulta de horario "+fecha_consulta2);
        //console.log("dia consulta de horario "+fecha_consulta2.substring(8, 10));
        $("#cmbHora2").empty();
        $.ajax({
                type: "GET",
                url: './buscarHorario?idAsunto='+$("#cmbAsunto2").val()+'&dia='+fecha_consulta2.substring(8, 10),
                success: function(response)
                {
                    $("#cmbHora2").html(response).fadeIn();
                }
      });
 });
    $("#cmbSeccion2").change(function() {
       // alert("id seccion "+$("#cmbSeccion").val());
        $("#cmbAsunto2").empty();
        $.ajax({
            type: "GET",
            url: './buscarAsunto?id='+$("#cmbSeccion2").val(),
            success: function(response)
            {
                $("#cmbAsunto2").html(response).fadeIn();
            }
      });
 
    });
});

</script>

