        function nobackbutton(){
            window.location.hash="no-back-button";
            window.location.hash="Again-No-back-button" //chrome
            window.onhashchange=function(){window.location.hash="no-back-button";}
        }
        function nobotonderecho(){
	       document.oncontextmenu = function(){alert ("SEDENA, TODOS LOS DERECHOS RESERVADOS.");return false}
        }
        function buscarAsuntoDeSeccion(selectObj) { 
                // get the index of the selected option 
                var idx = selectObj.selectedIndex; 
                // get the value of the selected option 
                var which = selectObj.options[idx].value;
                document.getElementById('f1').action="buscarAsunto";
                document.getElementById('f1').submit();
                /*if(which==="1"){
                    //alert("selecciono banco ");
                    document.getElementById('cmbBanco').style.display = 'block';
                    document.getElementById('etiBanco').style.display = 'block';
                }else{
                    document.getElementById('cmbBanco').style.display = 'none';
                    document.getElementById('etiBanco').style.display = 'none';
                }*/
                //alert("valor de select "+which);
        }
